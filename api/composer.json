{
    "type": "project",
    "name": "dgreda/dms",
    "license": "GNU GPL v3.0",
    "description": "Simple Document Management System in Symfony 4",
    "authors": [
        {
            "name": "Damian GrÄ™da",
            "email": "damian.greda@icloud.com",
            "homepage": "https://github.com/dgreda",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.64",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.6",
        "jms/serializer-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5",
        "sensiolabs/security-checker": "^4.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/validator": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.14.1",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/browser-kit": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "linkPhpUnitBridge": "ln -sf ../../bin/phpunit vendor/bin/phpunit",
        "post-install-cmd": [
            "@auto-scripts",
            "@linkPhpUnitBridge"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@linkPhpUnitBridge"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
